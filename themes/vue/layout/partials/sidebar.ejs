<div class="sidebar">
  <div class="sidebar-inner">
    <ul class="main-menu">
      <%- partial('partials/main_menu', { context: 'sidebar' }) %>
    </ul>
    <div class="list">
      <div class="main-sponsor">
        <span>Platinum Sponsorlar</span>
        <div>
        <%_ for (const sponsor of theme.platinum_sponsors) {_%>
        <a href="<%- sponsor.url %>" target="_blank" class="logo">
          <img src="<%- url_for(`/images/${sponsor.img}`) %>">
        </a>
        <%_ } _%>
        </div>
      </div>
      <a class="become-backer" href="<%- url_for("/support-vuejs") %>">
        Sponsor Ol
      </a>
      <h2>
        <%
        var typeText;
        if(type==='api'){
          typeText='API';
        }else if(type === 'guide'){
          typeText='Kılavuz';
        }else if(type === 'style-guide'){
          typeText='Yazım Standartları<sup class="beta">beta</sup>';
        }else if(type === 'cookbook'){
          typeText='Öneriler';
        }else if(type === 'examples'){
          typeText='Örnekler';
        }else{
          typeText=type;
        }
        %>
        <%-typeText%>
        <% if (['cookbook', 'style-guide'].indexOf(type) === -1) { %>
          <select class="version-select">
            <option value="SELF" selected>2.x</option>
            <option value="v1">1.0</option>
            <option value="012">0.12</option>
            <option value="011">0.11</option>
          </select>
        <% } %>
      </h2>
      <%- partial('partials/toc', { type: type }) %>
    </div>
  </div>
</div>
